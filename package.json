{
	"name": "nestjs-cqrs-extra",
	"description": "Library that provides additional features for the NestJS CQRS module.",
	"version": "1.1.9",
	"scripts": {
		"build": "rimraf -rf dist && tsc -p tsconfig.build.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky",
		"format": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
		"lint": "eslint --ignore-pattern .gitignore \"{src,apps,libs,test}/**/*.ts\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:ci": "jest --ci --passWithNoTests --coverage"
	},
	"lint-staged": {
		"*.ts": "npm run format"
	},
	"directories": {
		"lib": "src",
		"test": "test"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"nestjs",
		"cqrs",
		"nestjs-cqrs",
		"nestjs-cqrs-extra",
		"nats"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SocketSomeone/nestjs-cqrs-extra"
	},
	"bugs": {
		"url": "https://github.com/SocketSomeone/nestjs-cqrs-extra/issues"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"devDependencies": {
		"@commitlint/cli": "20.1.0",
		"@commitlint/config-angular": "20.0.0",
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@favware/npm-deprecate": "2.0.0",
		"@nestjs/common": "11.1.7",
		"@nestjs/core": "11.1.7",
		"@nestjs/microservices": "11.1.7",
		"@nestjs/testing": "11.1.7",
		"@release-it/conventional-changelog": "^10.0.0",
		"@types/jest": "30.0.0",
		"@types/node": "24.9.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "5.5.4",
		"globals": "^16.0.0",
		"husky": "9.1.7",
		"jest": "30.2.0",
		"lint-staged": "16.2.5",
		"nats": "^2.26.0",
		"prettier": "3.6.2",
		"reflect-metadata": "0.2.2",
		"release-it": "19.0.5",
		"rimraf": "6.0.1",
		"rxjs": "7.8.2",
		"ts-jest": "29.4.5",
		"ts-node": "10.9.2",
		"typescript": "5.9.3",
		"typescript-eslint": "^8.21.0"
	},
	"peerDependencies": {
		"@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
		"@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
		"@nestjs/microservices": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
		"nats": "*",
		"reflect-metadata": "^0.2.1",
		"rxjs": "^7.2.0"
	},
	"dependencies": {
		"tslib": "^2.8.1"
	}
}
